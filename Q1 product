def identity(n):
	prod=1
	while n>1:
		prod,n=prod*n,n-1
	return prod
  
def square(n):
	prod=1
	while n>1:
		prod,n=prod*n**2,n-1
	return prod
  
def triple(n):
	prod=1
	while n>1:
		prod,n=prod*n**3,n-1
	return prod

def increment(n):
	prod=1
	while n>1:
		prod,n=prod*(n+1),n-1
	return prod

def product(n,term):
	if term == identity:
		return identity(n)
  if term == square:
    return square(n)
  if term == triple:
    return triple(n) 
    if term == increment:
    return increment(n)
    
